local placeId = game.PlaceId

if placeId == 17738975314 then
    -- Philly Street Script
    loadstring(game:HttpGet("https://raw.githubusercontent.com/oz490/Philly-Street-Script/refs/heads/main/Philly%20Street%20Script%20ClicksFarm"))()
elseif placeId == 13643807539 then
    -- South Bronx Script
    loadstring(game:HttpGet("https://raw.githubusercontent.com/oz490/South-Bronx-lastest/main/Southbro"))()
else
    game.StarterGui:SetCore("SendNotification", {
        Title = "Game Check";
        Text = "FALSE GAME";
        Duration = 6; -- Time in seconds the notification stays on screen
    })
end
